Package: emodnet.bio.r.geo.tutorials
Title: EMODnet Biology Geospatial R Tutorials
Version: 0.0.0.9000
Authors@R: c(
    person("Anna", "Krystalli", , "annakrystalli@googlemail.com", role = c("aut", "cre"),
           comment = c(ORCID = "0000-0002-2378-4915")),
    person("EMODnet Biology", role = c("cph", "fnd")),
    person("European Commission DG MARE", role = "fnd")
